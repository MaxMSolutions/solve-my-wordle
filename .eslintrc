{
    "parser": "babel-eslint",
    "commaDangle": "on",
    "extends": "react",
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        "semi": "error",
        "max-len": [1, 150, 4]
    }
}
